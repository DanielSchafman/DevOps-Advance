#cron:
* * * * *

#script:
pipeline {
    agent any
    environment{
        GIT_REPO = 'https://github.com/DanielSchafman/DevOps-Advance.git'
    }
    stages {
        stage('clone repo') {
            steps {
                git branch: 'main', url: "${GIT_REPO}"
            }
        }
        stage('see folder'){
            steps{
                sh 'ls'                
            }
        }
    }
}

testing again